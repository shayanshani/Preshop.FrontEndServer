@{
    ViewBag.Title = "Home";
}
<script>
        $(function () {
            BindProducts()
            BindCategoriesOwlcarousel();
    });
    function BindProducts() {
        $.ajax({
                type: "GET",
                dataType: 'html',
            url: '@Url.Action("GetProducts", "Home")',
                success: function (data) {
                    var $ProductsDiv = $('#DivProductsOwlcarouselTop');
                    if (data == '')
                        $ProductsDiv.html("<div class='col-lg-12'>No Products found</div>");
                        else {
                        $ProductsDiv.html(data);
                        initOwlcarousel('#DivProductsOwlcarouselTop');
                    }
                }
            });
    }
    function GetSaleProducts() {
        $.ajax({
            type: "GET",
            dataType: 'html',
            url: '@Url.Action("GetSaleProducts", "Home")',
            success: function (data) {
                var $divSaleProducts= $('#DivSaleProductsOwlcarouselTop');
                if (data == '')
                    $divSaleProducts.html("<div class='col-lg-12'>No Products found</div>");
                else {
                    $divSaleProducts.html(data);
                    initOwlcarousel('#DivSaleProductsOwlcarouselTop');
                }
            }
        });
    }
    function BindCategoriesOwlcarousel() {
        $.ajax({
            type: "GET",
            dataType: 'html',
            url: '@Url.Action("GetCategoriesCarousel", "Home")',
            success: function (data) {
                var $divCategoriesCarousel = $('#DivCategoriesOwlcarousel');
                if (data == '')
                    $divCategoriesCarousel.html("<div class='col-lg-12'>No Categories found</div>");
                else {
                    $divCategoriesCarousel.html(data);
                    initOwlcarousel('#DivCategoriesOwlcarousel');
                }
            }
        });
    }
</script>
